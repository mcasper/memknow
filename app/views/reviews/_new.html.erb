<div class="review-buttons">
<%= form_for :review, url: flashcard_reviews_path(flashcard) do |f| %>
	<div class="col-md-3 col-xs-3">
		<%= f.submit ":(", class: "btn btn-danger" %>
	</div>
	<div class="col-md-3 col-xs-3">
		<%= f.submit "Ehhh", class: "btn btn-warning" %>
	</div>
	<div class="col-md-3 col-xs-3">
		<%= f.submit "Not bad", class: "btn btn-info" %>
	</div>
	<div class="col-md-3 col-xs-3">
		<%= f.submit "Aced", class: "btn btn-success" %>
	</div>
<% end %>
</div>
